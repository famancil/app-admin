<%= form_errors(@user.errors)%>
<%= form_for([:corporacion, @user], html: {role: "form",multipart: true, method: :put}) do |form| %>

  <div class="field">
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div class="field">
    <%= form.label :apellido %>
    <%= form.text_field :apellido %>
  </div>

  <div class="field">
    <%= form.label :rut %>
    <%= form.text_field :rut %>
  </div>

  <div class="field">
    <%= form.label :correo %>
    <%= form.text_field :correo %>
  </div>

  <div class="field">
    <%= form.label :contraseña %>
    <%= form.text_field :contraseña %>
  </div>

  <div class="field">
    <%= form.label :sucursal %>
    <%= form.text_field :sucursal %>
  </div>

  <div class="field">
    <%= form.label :filas %>
    <%= form.text_field :filas %>
  </div>

  <%= form.hidden_field :corporacion_id, value: @corporacion_id %>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
